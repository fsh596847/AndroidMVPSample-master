<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/colorActivityBg"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
  <RelativeLayout
      android:orientation="vertical"
      android:background="@color/colorMainTitleColor"
      android:layout_width="match_parent" android:layout_height="@dimen/activity_title_height">
    <TextView
        android:text="交货单位"
        android:onClick="backClick"
        style="@style/TextTitleStyle"
        android:drawableLeft="@mipmap/back"
    />

  </RelativeLayout>

  <com.fsh.zhaolong.swiperecyclerview.SwipeRecyclerView
      android:id="@+id/swipeRecyclerView"
      android:layout_width="match_parent"
      android:layout_height="match_parent"/>
</LinearLayout>